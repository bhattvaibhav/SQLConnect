@model IEnumerable<SQLConnect.Modal.Employee>

<h1>
    Employee List Ver2
</h1>

<table>
    @foreach (var obj in Model)
    {
    <tr>
        <td>
            Name
        </td>
        <td>
            @obj.Name
        </td>
        <td>
            Age
        </td>
        <td>
            @obj.Age
        </td>
        <td>
            City
        </td>
        <td>
            @obj.City
        </td>
    </tr>
    }
</table>